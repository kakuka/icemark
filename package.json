{
	"name": "icemark-agent",
	"displayName": "%extension.displayName%",
	"description": "%extension.description%",
	"publisher": "icemark-tech",
	"version": "1.2.9",
	"icon": "assets/icons/icemark-logo.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0",
		"node": "20.18.1"
	},
	"author": {
		"name": "Icemark Team"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kakuka/icemark"
	},
	"homepage": "https://icemark.tech",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"icemark",
		"product manager",
		"产品经理",
		"PRD",
		"prototype",
		"market research",
		"user research",
		"user analysis",
		"用户分析",
		"需求管理",
		"用户研究",
		"用户画像",
		"市场分析",
		"原型",
		"产品文档",
		"产品设计",
		"产品管理",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"writing",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"agentic"
	],
	"activationEvents": [
		"onLanguage",
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"submenus": [
			{
				"id": "icemark.contextMenu",
				"label": "%views.contextMenu.label%"
			},
			{
				"id": "icemark.terminalMenu",
				"label": "%views.terminalMenu.label%"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "icemark-ActivityBar",
					"title": "%views.activitybar.title%",
					"icon": "assets/icons/icemark-icon.svg"
				}
			]
		},
		"views": {
			"icemark-ActivityBar": [
				{
					"type": "webview",
					"id": "icemark.SidebarProvider",
					"name": "",
					"icon": "assets/icons/icemark-icon.svg"
				}
			]
		},
		"commands": [
			{
				"command": "icemark.plusButtonClicked",
				"title": "%command.newTask.title%",
				"icon": "$(add)"
			},
			{
				"command": "icemark.workspaceButtonClicked",
				"title": "%command.workspace.title%",
				"icon": "assets/images/workspace-close.svg"
			},
			{
				"command": "icemark.workspaceButtonClickedActive",
				"title": "%command.workspace.title%",
				"icon": "assets/images/workspace.svg"
			},
			{
				"command": "icemark.mcpButtonClicked",
				"title": "%command.mcpServers.title%",
				"icon": "$(server)"
			},
			{
				"command": "icemark.promptsButtonClicked",
				"title": "%command.prompts.title%",
				"icon": "$(notebook)"
			},
			{
				"command": "icemark.historyButtonClicked",
				"title": "%command.history.title%",
				"icon": "$(history)"
			},
			{
				"command": "icemark.popoutButtonClicked",
				"title": "%command.openInEditor.title%",
				"icon": "$(link-external)"
			},
			{
				"command": "icemark.settingsButtonClicked",
				"title": "%command.settings.title%",
				"icon": "$(settings-gear)"
			},
			{
				"command": "icemark.openInNewTab",
				"title": "%command.openInNewTab.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.explainCode",
				"title": "%command.explainCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.fixCode",
				"title": "%command.fixCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.improveCode",
				"title": "%command.improveCode.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.addToContext",
				"title": "%command.addToContext.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.newTask",
				"title": "%command.newTask.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.terminalAddToContext",
				"title": "%command.terminal.addToContext.title%",
				"category": "Terminal"
			},
			{
				"command": "icemark.terminalFixCommand",
				"title": "%command.terminal.fixCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "icemark.terminalExplainCommand",
				"title": "%command.terminal.explainCommand.title%",
				"category": "Terminal"
			},
			{
				"command": "icemark.setCustomStoragePath",
				"title": "%command.setCustomStoragePath.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.focusInput",
				"title": "%command.focusInput.title%",
				"category": "%configuration.title%"
			},
			{
				"command": "icemark.acceptInput",
				"title": "%command.acceptInput.title%",
				"category": "%configuration.title%"
			}
		],
		"menus": {
			"editor/context": [
				{
					"submenu": "icemark.contextMenu",
					"group": "navigation"
				}
			],
			"icemark.contextMenu": [
				{
					"command": "icemark.addToContext",
					"group": "1_actions@1"
				},
				{
					"command": "icemark.explainCode",
					"group": "1_actions@2"
				},
				{
					"command": "icemark.improveCode",
					"group": "1_actions@3"
				}
			],
			"terminal/context": [
				{
					"submenu": "icemark.terminalMenu",
					"group": "navigation"
				}
			],
			"icemark.terminalMenu": [
				{
					"command": "icemark.terminalAddToContext",
					"group": "1_actions@1"
				},
				{
					"command": "icemark.terminalFixCommand",
					"group": "1_actions@2"
				},
				{
					"command": "icemark.terminalExplainCommand",
					"group": "1_actions@3"
				}
			],
			"view/title": [
				{
					"command": "icemark.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == icemark.SidebarProvider"
				},
				{
					"command": "icemark.workspaceButtonClicked",
					"group": "navigation@2",
					"when": "view == icemark.SidebarProvider && !icemark.workspaceVisible"
				},
				{
					"command": "icemark.workspaceButtonClickedActive",
					"group": "navigation@2",
					"when": "view == icemark.SidebarProvider && icemark.workspaceVisible"
				},
				{
					"command": "icemark.promptsButtonClicked",
					"group": "navigation@3",
					"when": "view == icemark.SidebarProvider"
				},
				{
					"command": "icemark.mcpButtonClicked",
					"group": "navigation@4",
					"when": "view == icemark.SidebarProvider"
				},
				{
					"command": "icemark.historyButtonClicked",
					"group": "navigation@5",
					"when": "view == icemark.SidebarProvider"
				},
				{
					"command": "icemark.popoutButtonClicked",
					"group": "navigation@6",
					"when": "view == icemark.SidebarProvider"
				},
				{
					"command": "icemark.settingsButtonClicked",
					"group": "navigation@7",
					"when": "view == icemark.SidebarProvider"
				}
			],
			"editor/title": [
				{
					"command": "icemark.plusButtonClicked",
					"group": "navigation@1",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider"
				},
				{
					"command": "icemark.workspaceButtonClicked",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider && !icemark.workspaceVisible"
				},
				{
					"command": "icemark.workspaceButtonClickedActive",
					"group": "navigation@2",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider && icemark.workspaceVisible"
				},
				{
					"command": "icemark.promptsButtonClicked",
					"group": "navigation@3",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider"
				},
				{
					"command": "icemark.mcpButtonClicked",
					"group": "navigation@4",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider"
				},
				{
					"command": "icemark.historyButtonClicked",
					"group": "navigation@5",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider"
				},
				{
					"command": "icemark.popoutButtonClicked",
					"group": "navigation@6",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider"
				},
				{
					"command": "icemark.settingsButtonClicked",
					"group": "navigation@7",
					"when": "activeWebviewPanelId == icemark.TabPanelProvider"
				}
			]
		},
		"configuration": {
			"title": "%configuration.title%",
			"properties": {
				"icemark.allowedCommands": {
					"type": "array",
					"items": {
						"type": "string"
					},
					"default": [
						"npm test",
						"npm install",
						"tsc",
						"git log",
						"git diff",
						"git show"
					],
					"description": "%commands.allowedCommands.description%"
				},
				"icemark.vsCodeLmModelSelector": {
					"type": "object",
					"properties": {
						"vendor": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.vendor.description%"
						},
						"family": {
							"type": "string",
							"description": "%settings.vsCodeLmModelSelector.family.description%"
						}
					},
					"description": "%settings.vsCodeLmModelSelector.description%"
				},
				"icemark.customStoragePath": {
					"type": "string",
					"default": "",
					"description": "%settings.customStoragePath.description%"
				}
			}
		}
	},
	"scripts": {
		"build": "npm run vsix",
		"build:webview": "cd webview-ui && npm run build",
		"build:esbuild": "node esbuild.js --production",
		"compile": "tsc -p . --outDir out && node esbuild.js",
		"install:all": "npm install npm-run-all && npm-run-all -l -p install-*",
		"install-extension": "npm install",
		"install-webview": "cd webview-ui && npm install",
		"install-e2e": "cd e2e && npm install",
		"lint": "npm-run-all -l -p lint:*",
		"lint:extension": "eslint src --ext .ts",
		"lint:webview": "cd webview-ui && npm run lint",
		"lint:e2e": "cd e2e && npm run lint",
		"check-types": "npm-run-all -l -p check-types:*",
		"check-types:extension": "tsc --noEmit",
		"check-types:webview": "cd webview-ui && npm run check-types",
		"check-types:e2e": "cd e2e && npm run check-types",
		"package": "npm-run-all -l -p build:webview build:esbuild check-types lint",
		"pretest": "npm run compile",
		"dev": "cd webview-ui && npm run dev",
		"test": "node scripts/run-tests.js",
		"test:extension": "jest -w=40%",
		"test:webview": "cd webview-ui && npm run test",
		"prepare": "husky",
		"publish:marketplace": "vsce publish && ovsx publish",
		"publish": "npm run build && changeset publish && npm install --package-lock-only",
		"version-packages": "changeset version && npm install --package-lock-only",
		"vscode:prepublish": "npm run package",
		"vsix": "rimraf bin && mkdirp bin && npx vsce package --out bin",
		"watch": "npm-run-all -l -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"watch-tests": "tsc -p . -w --outDir out",
		"changeset": "changeset",
		"knip": "knip --include files",
		"clean": "npm-run-all -l -p clean:*",
		"clean:extension": "rimraf bin dist out",
		"clean:webview": "cd webview-ui && npm run clean",
		"clean:e2e": "cd e2e && npm run clean",
		"vscode-test": "npm-run-all -l -p vscode-test:*",
		"vscode-test:extension": "tsc -p . --outDir out && node esbuild.js",
		"vscode-test:webview": "cd webview-ui && npm run build",
		"update-contributors": "node scripts/update-contributors.js",
		"generate-types": "tsx scripts/generate-types.mts",
		"icemark-action": "node scripts/icemark-action.js"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.2",
		"@anthropic-ai/sdk": "^0.37.0",
		"@anthropic-ai/vertex-sdk": "^0.7.0",
		"@aws-sdk/client-bedrock-runtime": "^3.779.0",
		"@google/genai": "^0.12.0",
		"@mistralai/mistralai": "^1.3.6",
		"@modelcontextprotocol/sdk": "^1.11.1",
		"@types/clone-deep": "^4.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/tmp": "^0.2.6",
		"@types/turndown": "^5.0.5",
		"@types/vscode": "^1.95.0",
		"@vscode/codicons": "^0.0.36",
		"adm-zip": "^0.5.16",
		"axios": "^1.7.4",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.1",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"diff-match-patch": "^1.0.5",
		"express": "^4.21.1",
		"fast-deep-equal": "^3.1.3",
		"fast-xml-parser": "^4.5.1",
		"fastest-levenshtein": "^1.0.16",
		"fzf": "^0.5.2",
		"get-folder-size": "^5.0.0",
		"i18next": "^24.2.2",
		"isbinaryfile": "^5.0.2",
		"mammoth": "^1.8.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"node-cache": "^5.1.2",
		"node-ipc": "^12.0.0",
		"officeparser": "^5.1.1",
		"openai": "^4.78.1",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"pkce-challenge": "^4.1.0",
		"posthog-node": "^4.7.0",
		"pretty-bytes": "^6.1.1",
		"ps-tree": "^1.2.0",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.4.0",
		"reconnecting-eventsource": "^1.6.4",
		"say": "^0.16.0",
		"serialize-error": "^11.0.3",
		"simple-git": "^3.27.0",
		"sound-play": "^1.1.0",
		"string-similarity": "^4.0.4",
		"strip-ansi": "^7.1.0",
		"strip-bom": "^5.0.0",
		"tiktoken": "^1.0.21",
		"tmp": "^0.2.3",
		"tree-sitter-wasms": "^0.1.11",
		"turndown": "^7.2.0",
		"vscode-material-icons": "^0.1.1",
		"web-tree-sitter": "^0.22.6",
		"workerpool": "^9.2.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.10",
		"@changesets/types": "^6.0.0",
		"@dotenvx/dotenvx": "^1.34.0",
		"@types/adm-zip": "^0.5.5",
		"@types/debug": "^4.1.12",
		"@types/diff": "^5.2.1",
		"@types/diff-match-patch": "^1.0.36",
		"@types/express": "^5.0.2",
		"@types/glob": "^8.1.0",
		"@types/jest": "^29.5.14",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@types/node-cache": "^4.1.3",
		"@types/node-ipc": "^9.2.3",
		"@types/ps-tree": "^1.1.6",
		"@types/string-similarity": "^4.0.2",
		"@typescript-eslint/eslint-plugin": "^7.14.1",
		"@typescript-eslint/parser": "^7.11.0",
		"@vscode/test-electron": "^2.5.2",
		"@vscode/vsce": "^3.3.2",
		"esbuild": "^0.24.0",
		"eslint": "^8.57.0",
		"execa": "^9.5.2",
		"glob": "^11.0.1",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"jest-simple-dot-reporter": "^1.0.5",
		"knip": "^5.44.4",
		"lint-staged": "^15.2.11",
		"mkdirp": "^3.0.1",
		"nock": "^14.0.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.4.2",
		"rimraf": "^6.0.1",
		"ts-jest": "^29.2.5",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"typescript": "^5.4.5",
		"zod-to-ts": "^1.2.0"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx,json,css,md}": [
			"prettier --write"
		],
		"src/**/*.{ts,tsx}": [
			"npx eslint -c .eslintrc.json --max-warnings=0 --fix"
		],
		"webview-ui/**/*.{ts,tsx}": [
			"npx eslint -c webview-ui/.eslintrc.json --max-warnings=0 --fix"
		]
	}
}
